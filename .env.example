# 環境設置
ENV=development

# API 設置
PORT=8890
HOST=0.0.0.0

# 安全設置
API_KEY=your_api_key_here
CORS_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# 性能設置
WORKERS=4
MAX_REQUESTS=10000
TIMEOUT=300

# 日誌設置
LOG_LEVEL=info
LOG_FORMAT=json  # 或 text

# 監控設置
ENABLE_METRICS=true
PROMETHEUS_MULTIPROC_DIR=/tmp 